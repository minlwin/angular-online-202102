{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "790ad3fb-9471-4a13-8611-387c55612264"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "e01-injection": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/e01-injection",
      "sourceRoot": "projects/e01-injection/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/e01-injection",
            "index": "projects/e01-injection/src/index.html",
            "main": "projects/e01-injection/src/main.ts",
            "polyfills": "projects/e01-injection/src/polyfills.ts",
            "tsConfig": "projects/e01-injection/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/e01-injection/src/favicon.ico",
              "projects/e01-injection/src/assets"
            ],
            "styles": [
              "projects/e01-injection/src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/e01-injection/src/environments/environment.ts",
                  "with": "projects/e01-injection/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "e01-injection:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "e01-injection:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "e01-injection:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/e01-injection/src/test.ts",
            "polyfills": "projects/e01-injection/src/polyfills.ts",
            "tsConfig": "projects/e01-injection/tsconfig.spec.json",
            "karmaConfig": "projects/e01-injection/karma.conf.js",
            "assets": [
              "projects/e01-injection/src/favicon.ico",
              "projects/e01-injection/src/assets"
            ],
            "styles": [
              "projects/e01-injection/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/e01-injection/tsconfig.app.json",
              "projects/e01-injection/tsconfig.spec.json",
              "projects/e01-injection/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/e01-injection/e2e/protractor.conf.js",
            "devServerTarget": "e01-injection:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "e01-injection:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "e01-injection"
}